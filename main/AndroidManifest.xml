<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.emudev.n64"
      android:versionCode="9"
      android:versionName="0.1"
      android:installLocation="auto">
    <application android:label="@string/app_name" android:icon="@drawable/icon" android:launchMode="singleTask">
        <activity android:name=".MainActivity"
                  android:label="@string/app_name"
                  android:screenOrientation="landscape"
                  android:configChanges="orientation|keyboardHidden|uiMode">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuActivity"
                  android:label="Main Menu">
        </activity>
        <activity android:name=".MenuSettingsActivity"
                  android:label="Settings">
        </activity>
        <activity android:name=".MenuSettingsVideoActivity"
                  android:label="Video Settings">
        </activity>
        <activity android:name=".MenuSettingsVideoChangeActivity"
                  android:label="Choose Video Plug-in">
        </activity>
        <activity android:name=".MenuSettingsVideoConfigureActivity"
                  android:label="Configure Video Plug-in">
        </activity>
        <activity android:name=".MenuSettingsAudioActivity"
                  android:label="Audio Settings">
        </activity>
        <activity android:name=".MenuSettingsAudioChangeActivity"
                  android:label="Choose Audio Plug-in">
        </activity>
        <activity android:name=".MenuSettingsInputActivity"
                  android:label="Input Settings">
        </activity>
        <activity android:name=".MenuSettingsInputChangeActivity"
                  android:label="Choose Input Plug-in">
        </activity>
        <activity android:name=".MenuSettingsInputConfigureActivity"
                  android:label="Map Controller Buttons">
        </activity>
        <activity android:name=".MenuSettingsRSPActivity"
                  android:label="RSP Settings">
        </activity>
        <activity android:name=".MenuSettingsRSPChangeActivity"
                  android:label="Choose RSP Plug-in">
        </activity>
        <activity android:name=".MenuSettingsCoreActivity"
                  android:label="Core Settings">
        </activity>
        <activity android:name=".MenuSettingsCoreChangeActivity"
                  android:label="Choose Core">
        </activity>
        <activity android:name=".MenuSettingsCoreConfigureActivity"
                  android:label="Configure Core">
        </activity>
        <activity android:name=".MenuSkinsActivity"
                  android:label="Skins (look and feel)">
        </activity>
        <activity android:name=".MenuSkinsGamepadActivity"
                  android:label="Virtual Gamepad Settings">
        </activity>
        <activity android:name=".MenuSkinsGamepadChangeActivity"
                  android:label="Select a Layout">
        </activity>
        <activity android:name=".FileChooserActivity"
                  android:label="@string/app_name">
        </activity>
        <activity android:name=".SDLActivity"
                  android:label="@string/app_name"
                  android:screenOrientation="landscape"
                  android:configChanges="orientation|keyboardHidden|uiMode">
        </activity>
    </application>
    <uses-feature android:glEsVersion="0x00020000"/>
    <uses-sdk android:minSdkVersion="4" android:targetSdkVersion="9"/>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE"></uses-permission>
</manifest> 
